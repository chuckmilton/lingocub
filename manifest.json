{
    "manifest_version": 3,
    "name": "LingoCub",
    "version": "1.0",
    "description": "Dub YouTube videos in real-time using ElevenLabs API.",
    "permissions": ["storage", "tabs", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  